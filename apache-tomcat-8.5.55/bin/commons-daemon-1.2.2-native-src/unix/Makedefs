CC = gcc
CFLAGS = -g -O2 -DOS_DARWIN -DDSO_DLFCN -DCPU=\"amd64\" -Wall -Wstrict-prototypes
CPPFLAGS = 
INCLUDES =  -I/Library/Java/JavaVirtualMachines/jdk1.8.0_121.jdk/Contents/Home/include -I/Library/Java/JavaVirtualMachines/jdk1.8.0_121.jdk/Contents/Home//include -I/Library/Java/JavaVirtualMachines/jdk1.8.0_121.jdk/Contents/Home//include/darwin
LDFLAGS = 
LIBS = 
RANLIB = ranlib
LDCMD = gcc
STRIP = @: 

.c.o:
	$(CC) $(CFLAGS) $(CPPFLAGS) $(EXTRA_CFLAGS) $(INCLUDES) -c $< -o $@
